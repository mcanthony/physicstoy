												<option>2</option>
												<option>4</option>
												<option>8</option>
												<option>16</option>
												<option>32</option>
												<option>64</option>
											</select>
										</div>
									</div>

									<div class="form-group">
										<label class="col-sm-4 control-label">mask</label>
