								<div class="form-horizontal">

									<ol class="breadcrumb">
										<li><a href="#" ng-click="setSelectedId(-1)">World</a></li>
										<li><a href="#" ng-click="setSelectedId(body.id)">{{body.name}}</a></li>
										<li class="active">{{shape.name}}</li>
