											<input type="number" class="form-control" ng-model="shape.x"/>
										</div>
										<div class="col-sm-4">
											<input type="number" class="form-control" ng-model="shape.y"/>
										</div>
									</div>

									<div class="form-group">
										<label class="col-sm-4 control-label">angle</label>
